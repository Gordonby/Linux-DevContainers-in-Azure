{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourceGroup": {
      "value": "development-test"
    },
    "defaultPrefix": {
      "value": "dev4"
    },
    "location": {
      "value": "uksouth"
    },
    "vnetAddressPrefix": {
      "value": "10.1.0.0/16"
    },
    "subNets": {
      "value": [
        {
          "name": "GatewaySubnet",
          "addressSpace": "10.1.255.0/27",
          "securityRules": [],
          "delegations": []
        },
        {
          "name": "shared",
          "addressSpace": "10.1.0.0/24",
          "securityRules": [
            {
              "name": "Internal-Only",
              "properties": {
                "priority": 200,
                "access": "Allow",
                "direction": "Inbound",
                "protocol": "Tcp",
                "sourcePortRange": "*",
                "sourceAddressPrefix": "VirtualNetwork",
                "destinationAddressPrefix": "*",
                "destinationPortRange": "*"
              }
            }
          ],
          "delegations": []
        },
        {
          "name": "customdns",
          "addressSpace": "10.1.3.0/24",
          "securityRules": [
            {
              "name": "Internal-Only",
              "properties": {
                "priority": 200,
                "access": "Allow",
                "direction": "Inbound",
                "protocol": "Tcp",
                "sourcePortRange": "*",
                "sourceAddressPrefix": "VirtualNetwork",
                "destinationAddressPrefix": "*",
                "destinationPortRange": "*"
              }
            }
          ],
          "delegations": [
            {
              "name": "delegationService",
              "properties": {
                "serviceName": "Microsoft.ContainerInstance/containerGroups"
              }
            }
          ]
        },
        {
          "name": "backend",
          "addressSpace": "10.1.2.0/24",
          "securityRules": [
            {
              "name": "Internal-Only",
              "properties": {
                "priority": 200,
                "access": "Allow",
                "direction": "Inbound",
                "protocol": "Tcp",
                "sourcePortRange": "*",
                "sourceAddressPrefix": "VirtualNetwork",
                "destinationAddressPrefix": "*",
                "destinationPortRange": "*"
              }
            }
          ],
          "delegations": []
        },
        {
          "name": "frontend",
          "addressSpace": "10.1.1.0/24",
          "securityRules": [
            {
              "name": "Internal-Only",
              "properties": {
                "priority": 200,
                "access": "Allow",
                "direction": "Inbound",
                "protocol": "Tcp",
                "sourcePortRange": "*",
                "sourceAddressPrefix": "VirtualNetwork",
                "destinationAddressPrefix": "*",
                "destinationPortRange": "*"
              }
            }
          ],
          "delegations": []
        }
      ]
    },
    "aci": {
      "value": {
        "image": "coredns/coredns:latest",
        "port": 53,
        "cpuCores": 1,
        "memoryInGb": 0.5,
        "protocol": "UDP",
        "restartPolicy": "Always",
        "gitrepourl": "https://github.com/Sam-Rowe/Remote-DevContainers-Extras",
        "gitrepomountpath": "/config",
        "commandline": "/coredns -conf /config/host/$corednsFile",
        "subnet": "customdns"
      }
    },
    "vpn": {
      "value": {
        "sku": "Basic",
        "type": "Vpn",
        "addressPrefix": "172.16.24.0/24",
        "generation": "Generation1"
      }
    }
  }
}