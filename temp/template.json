{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "dnsResolvers_dnsresolver_name": {
            "defaultValue": "dnsresolver",
            "type": "String"
        },
        "virtualNetworks_vnet_vscode_prod_externalid": {
            "defaultValue": "/subscriptions/461377a7-433d-4980-9506-c35defb10a49/resourceGroups/myvscode/providers/Microsoft.Network/virtualNetworks/vnet-vscode-prod",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Network/dnsResolvers",
            "apiVersion": "2020-04-01-preview",
            "name": "[parameters('dnsResolvers_dnsresolver_name')]",
            "location": "uksouth",
            "properties": {
                "virtualNetwork": {
                    "id": "[parameters('virtualNetworks_vnet_vscode_prod_externalid')]"
                }
            }
        },
        {
            "type": "Microsoft.Network/dnsResolvers/inboundEndpoints",
            "apiVersion": "2020-04-01-preview",
            "name": "[concat(parameters('dnsResolvers_dnsresolver_name'), '/inboundendpoint')]",
            "location": "uksouth",
            "dependsOn": [
                "[resourceId('Microsoft.Network/dnsResolvers', parameters('dnsResolvers_dnsresolver_name'))]"
            ],
            "properties": {
                "ipConfigurations": [
                    {
                        "subnet": {
                            "id": "[concat(parameters('virtualNetworks_vnet_vscode_prod_externalid'), '/subnets/dnsresolver')]"
                        },
                        "privateIpAddress": "10.2.0.20",
                        "privateIpAllocationMethod": "Dynamic"
                    }
                ]
            }
        }
    ]
}